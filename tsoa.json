{
  "entryFile": "src/server.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "spec": {
    "outputDirectory": "public",
    "specVersion": 3,
    "securityDefinitions": {
        "api_key": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        },
       "BearerAuth":{
          "type": "http",
          "scheme": "bearer"
       }
    },
    "tags": [
      {
        "name": "Users",
        "description": "Apis about users management"
      },
      {
        "name": "Auth",
        "description": "Apis to authenticate user"
      }
    ]
  }
}